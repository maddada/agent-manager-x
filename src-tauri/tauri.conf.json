{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Agent Manager X",
  "version": "1.0.0",
  "identifier": "com.agent-manager-x",
  "build": {
    "beforeDevCommand": "pnpm run dev:vite",
    "devUrl": "http://localhost:1533",
    "beforeBuildCommand": "pnpm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Agent Manager X",
        "width": 1100,
        "height": 700,
        "minWidth": 700,
        "minHeight": 400,
        "zoomHotkeysEnabled": true,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "decorations": true,
        "center": true,
        "titleBarStyle": "Overlay",
        "trafficLightPosition": {
          "x": 14,
          "y": 30
        },
        "hiddenTitle": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
