{
  "name": "agent-manager-x",
  "private": true,
  "version": "0.1.13",
  "type": "module",
  "scripts": {
    "link:electrobun": "bash ./scripts/link-electrobun.sh",
    "dev": "bun run build && bash ./scripts/run-electrobun-dev.sh",
    "dev:hmr": "bash ./scripts/dev-hmr.sh",
    "hmr": "vite --port 1533",
    "build:frontend": "bun run link:electrobun && tsc && vite build",
    "build": "bun run link:electrobun && bun run build:frontend && node ../electrobun/package/bin/electrobun.cjs build",
    "build:prod": "bun run link:electrobun && bun run build:frontend && node ../electrobun/package/bin/electrobun.cjs build --channel prod",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "icons:build": "./scripts/build-macos-icons.sh",
    "icons:inject": "./scripts/inject-assets-car.sh"
  },
  "dependencies": {
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.555.0",
    "react": "^19.1.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.0",
    "react-masonry-css": "^1.0.16",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "jsdom": "^27.2.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vitest": "^4.0.15"
  }
}
